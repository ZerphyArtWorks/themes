  0% {-webkit-box-shadow: 0 0 0 0 var(--accent);}
  70% {-webkit-box-shadow: 0 0 0 5px rgba(204, 169, 44, 0);}
  100% {-webkit-box-shadow: 0 0 0 0 rgba(204, 169, 44, 0);}
}
